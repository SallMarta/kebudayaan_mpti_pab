<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorAccent</item>
        <item name="colorPrimaryDark">@color/colorAccent</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowLightStatusBar" tools:ignore="NewApi">true</item>
    </style>

    <style name="AppTheme.BackUpButton" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/blackColor</item>
        <item name="colorPrimaryDark">@color/blackColor</item>
        <item name="colorAccent">@color/toscaColor</item>
    </style>
    
    <style name="AppTheme.Splash" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/toscaColor</item>
        <item name="colorPrimaryDark">@color/toscaColor</item>
        <item name="colorAccent">@color/toscaColor</item>
    </style>



</resources>