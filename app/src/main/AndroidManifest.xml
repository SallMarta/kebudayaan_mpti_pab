<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kebudayaan_mpti_pab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Catatan_Data"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".QuizActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".StartingScreenActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".deskripsiBudaya2"
            android:screenOrientation="portrait" />

        <activity
            android:name=".activity_deskripsiBudaya"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity_pakaianAdat"
            android:parentActivityName=".activity_pengenalanBudaya"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity_pengenalanBudaya" />
        </activity>

        <activity
            android:name=".activity_rumahAdat"
            android:parentActivityName=".activity_pengenalanBudaya"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity_pengenalanBudaya" />
        </activity>

        <activity
            android:name=".activity_senjata_tradisional"
            android:parentActivityName=".activity_pengenalanBudaya"

            android:screenOrientation="portrait"
            android:label="Senjata Tradisional">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity_pengenalanBudaya" />
        </activity>

        <activity
            android:name=".activity_tarianAdat"
            android:parentActivityName=".activity_pengenalanBudaya"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity_pengenalanBudaya" />
        </activity>

        <activity
            android:name=".activity_pengenalanBudaya"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".SplashScreen"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />

    </application>
</manifest>